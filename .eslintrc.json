{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard-with-typescript",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",    
    "react-app",
    "prettier"
